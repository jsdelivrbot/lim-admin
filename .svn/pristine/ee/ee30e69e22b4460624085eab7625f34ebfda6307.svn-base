.yali-floater-box.skin-type-1 {position: fixed; right: 10px; vertical-align: middle;}
.yali-floater-box.skin-type-1 .yali-btn-group {transition: all 0.5s;opacity:1;}
.yali-floater-box.skin-type-1 .yali-btn-group li{margin: 5px 0; vertical-align: middle;  text-align:center;}
.yali-floater-box.skin-type-1 .yali-btn-group  i{display:block;font-size: 24px;}
.yali-floater-box.skin-type-1 button{width: 70px;height: 60px ;padding:10px 5px; cursor: pointer; color: #fff; border: 0; }
.yali-floater-box.skin-type-1 .yali-floater-colspan {background-color:rgba(146,145,145,0.7); line-height: 40px; font-size: 16px; text-align: center;}
.yali-floater-box.skin-type-1 .yali-floater-colspan:before {content:'客服'}
.yali-floater-box.skin-type-1 .yali-floater-colspan i{ display:inline-block; transform:rotate(90deg); transition: all 0.5s;}
.yali-floater-box.skin-type-1.colspan .yali-btn-group{ opacity:0;}
.yali-floater-box.skin-type-1.colspan .yali-floater-colspan i{ transform:rotate(270deg); }

.yali-floater-box.skin-type-2 {position: fixed; right: 10px; vertical-align: middle;transition: all 0.5s;}
.yali-floater-box.skin-type-2 .yali-btn-group {transition: right 0.5s;}
.yali-floater-box.skin-type-2 .yali-btn-group li{margin: 5px 0; vertical-align: middle;}
.yali-floater-box.skin-type-2 .yali-btn-group  i{display:inline-block; font-size: 16px;}
.yali-floater-box.skin-type-2 button{box-sizing: border-box; width: 120px; padding:10px; font-size: 14px; text-align: left; cursor: pointer; color: #fff; border: 0;}
.yali-floater-box.skin-type-2 .yali-floater-colspan {position: absolute; width: 140px; top: -50px; left: 0px; transition: all 0.5s; color: #666; background: #dddddd}
.yali-floater-box.skin-type-2 .yali-floater-colspan:after {content:'  在线客服'}
.yali-floater-box.skin-type-2 .yali-floater-colspan i{ display:inline-block; transform:rotate(0deg); transition: all 0.5s;}
.yali-floater-box.skin-type-2.colspan{ right:-140px;}
.yali-floater-box.skin-type-2.colspan .yali-floater-colspan {left: -25px;}
.yali-floater-box.skin-type-2.colspan .yali-floater-colspan i{ transform:rotate(180deg); }

.yali-floater-box.skin-type-3 {position: fixed; right: -80px; vertical-align: middle;}
.yali-floater-box.skin-type-3 .yali-btn-group {transition: all 0.5s;opacity:1;}
.yali-floater-box.skin-type-3 .yali-btn-group li{margin: 5px 0; vertical-align: middle; }
.yali-floater-box.skin-type-3 .yali-btn-group  i{display:inline-block; height: 50px; width:50px;text-align: center; font-size: 24px;}
.yali-floater-box.skin-type-3 button{width: 130px; height: 50px; line-height: 48px;  padding:2px; cursor: pointer; color: #fff; border: 0;  text-align:left;}
.yali-floater-box.skin-type-3 .yali-btn-group li:hover {margin-left:-80px; transition: all 0.5s;}
.yali-floater-box.skin-type-3 .yali-floater-colspan {background-color:rgba(146,145,145,0.7);padding:2px 2px 2px 12px;}
.yali-floater-box.skin-type-3 .yali-floater-colspan i{ display:inline-block;transform:rotate(90deg); transition: all 0.5s; font-size: 24px;}
.yali-floater-box.skin-type-3.colspan .yali-btn-group{ opacity:0;}
.yali-floater-box.skin-type-3.colspan .yali-floater-colspan i{ transform:rotate(270deg); }

.yali-floater-box.skin-type-4 {position: fixed; right: 10px; vertical-align: middle;}
.yali-floater-box.skin-type-4 .yali-btn-group {transition: all 0.8s; opacity:0; position: absolute; right:-80px; bottom: 60px;}
.yali-floater-box.skin-type-4 .yali-btn-group li{margin: 5px 0; vertical-align: middle;  text-align:center;}
.yali-floater-box.skin-type-4 .yali-btn-group li:hover {transition-duration: 1s;opacity: 0.6}
.yali-floater-box.skin-type-4 .yali-btn-group  i{display:block ;font-size: 24px;}
.yali-floater-box.skin-type-4 button{width: 70px;height: 60px ;padding:10px 5px; cursor: pointer; color: #fff; border: 0; }
.yali-floater-box.skin-type-4 .yali-floater-colspan {background-color:rgba(146,145,145,0.7);width: 30px;height: 60px ;padding:3px; text-align: center;}
.yali-floater-box.skin-type-4 .yali-floater-colspan:before {content:'客服'}
.yali-floater-box.skin-type-4 .yali-floater-colspan i{ display:inline-block; transform:rotate(0deg); transition: all 0.5s;}
.yali-floater-box.skin-type-4.colspan .yali-floater-colspan i{ transform:rotate(-180deg); }
.yali-floater-box.skin-type-4.colspan .yali-btn-group {transition: all 0.8s;opacity:1; right: 35px;}

