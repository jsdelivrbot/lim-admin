html #layuicss-plugslayuicss{display:none; position: absolute; width:1989px;}
body{overflow-x:hidden;}
/** 初始化 **/

.yali-im ul{padding: 20px; list-style: none;}
.yali-im button { vertical-align: middle; padding: 12px; margin: 2px; width: 130px; font-size: 14px; text-align: left;}
.yali-im button i{font-size: 24px;}

.lim {cursor: pointer;}
.lim.yali-btn-disabled, .lim.yali-disabled, .lim.disabled { border: 1px solid #e6e6e6; background-color: #FBFBFB; color: #C9C9C9;  cursor: not-allowed;}

.color-inline-1, .color-inline-2, .color-block-1, .color-block-2 {padding: 20px; box-shadow:  none;}
.color-inline-1 .yali-im li, .color-inline-2 .yali-im li{display: inline-block;}
.color-block-1 .yali-im li, .color-block-2 .yali-im li{display: block;}
.color-inline-1 .yali-im, .color-block-1 .yali-im {background: #666666; border: 1px solid #F2F2F2;}
.color-inline-2 .yali-im, .color-block-2 .yali-im {background: #F2F2F2; border: 1px solid #EEEEEE;}
.color-inline-1 .yali-im button, .color-block-1 .yali-im button {background: #FFF; color:#333; }
.color-inline-2 .yali-im button, .color-block-2 .yali-im button {background: #333; color:#FFF; }



.layim-chat .layim-chat-textarea {margin-right: 10px; height: 122px;}
.layim-chat .layui-layim-audio, .layim-chat .layui-layim-video, .layim-chat .layui-layim-file {max-width: 85px; display: inline-block; overflow: hidden; max-height: 120px; overflow: hidden;}
.layim-chat .layui-layim-photos {max-height: 80%; display: inline-block; overflow: hidden; cursor: pointer}
.layim-chat .layim-chat-send {right: 10px;}
.layim-chat .layim-chat-tool {color: #888;}


/*layim 外挂CSS 评分*/
.layui-layim-marking {margin: 10px 0; padding: 10px; background: #FFF; text-align: left; border-radius: 10px;}
.layui-layim-marking i {padding-right:5px; cursor: pointer; font-size: 24px;}
.layui-layim-marking cite {display: block; line-height: 30px;}


.box-padding {padding: 10px;}
.box-margin {margin: 10px;}

.box-padding-20 {padding: 20px;}
.box-margin-20 {margin: 20px;}

.yali-show {display: block !important;}
.yali-hidden {display: none !important;}
.yali-invisible {visibility: hidden;}




.text-danger{color:#FF4747}
.text-right {text-align: right;}
.text-center {text-align: center; vertical-align: middle; margin:0 auto;}
.select-table {border:#8391a5; margin:0 auto;}
.select-table span{height: 22px;width: 22px;display: block;padding-top:2px;margin-top: 5px;text-align: center;}
.select-table .el-select-dropdown__item{padding-top: 0px; padding-bottom: 0px;}

.skin-type-1{box-sizing: border-box;}
.skin-type-2{box-sizing: border-box; padding:9px; border: 1px solid #dddddd; border-radius:5px;}
.skin-type-3{box-sizing: border-box; font-size: 14px;}
.skin-type-4{}

.button-color-1{color: #fff;/*浅蓝*/ background: -webkit-linear-gradient(left top, #0091FF , #3AA9FD);}
.button-color-2{color: #fff;/*橙色*/ background: -webkit-linear-gradient(left top, #FF6C05 , #FFA334);}
.button-color-3{color: #fff;/*深蓝*/ background: -webkit-linear-gradient(left top, #025FC1 , #2C76C3);}
.button-color-4{color: #fff;/*梅紫*/ background: -webkit-linear-gradient(left top, #EA0450 , #E43870);}
.button-color-5{color: #fff;/*绿色*/ background: -webkit-linear-gradient(left top, #03C13E , #29C359);}
.button-color-6{color: #fff;/*橘黄*/ background: -webkit-linear-gradient(left top, #F79A2F , #F9B669);}
.button-color-7{color: #fff;/*玫粉*/ background: -webkit-linear-gradient(left top, #FD3D3B , #FD6A67);}
.button-color-8{color: #fff;/*大红*/ background: -webkit-linear-gradient(left top, #FF0101 , #FF3B37);}

.icon-block-1{color: #483D8B; }
.icon-block-2{color: #483D8B; }
.icon-block-3{color: #483D8B; }
.icon-block-4{color: #483D8B; }

